#: ../src/Rule/ChangeWithIntervalRule.php:156
msgid "There must be at least %interval% between password changes."
msgstr "Zwischen Passwortänderungen müssen mindestens %interval% liegen."

#: ../src/Rule/ChangeWithIntervalRule.php:167
msgid "There must be at most %interval% between password changes."
msgstr "Zwischen Passwortänderungen müssen maximal %interval% liegen."

#: ../src/Rule/ChangeWithIntervalRule.php:174
msgid "There must be exactly %interval% between password changes."
msgstr "Zwischen Passwortänderungen müssen genau %interval% liegen."

#: ../src/Rule/ChangeWithIntervalRule.php:180
msgid "There must be between %min% and %max% between password changes."
msgstr "Zwischen Passwortänderungen müssen zwischen %min% und %max% liegen."

#: ../src/Rule/DictionaryRule.php:117
msgid "The password cannot contain dictionary words."
msgstr "Das Passwort darf keine Wörter aus dem Wörterbuch enthalten."

#: ../src/Rule/DigitRule.php:25
msgid "The password must contain at least one digit."
msgid_plural "The password must contain at least %count% digits."
msgstr[0] "Das Passwort muss mindestens eine Zahl enthalten."
msgstr[1] "Das Passwort muss mindestens %count% Zahlen enthalten."

#: ../src/Rule/DigitRule.php:33
msgid "The password cannot contain digits."
msgstr "Das Passwort darf keine Zahl enthalten."

#: ../src/Rule/DigitRule.php:39
msgid "The password must contain at most one digit."
msgid_plural "The password must contain at most %count% digits."
msgstr[0] "Das Passwort darf maximal eine Zahl enthalten."
msgstr[1] "Das Passwort darf maximal %count% Zahlen enthalten."

#: ../src/Rule/DigitRule.php:47
msgid "The password must contain exactly one digit."
msgid_plural "The password must contain exactly %count% digits."
msgstr[0] "Das Passwort muss genau eine Zahl enthalten."
msgstr[1] "Das Passwort muss genau %count% Zahlen enthalten."

#: ../src/Rule/DigitRule.php:54
msgid "The password must contain between %min% and %max% digits."
msgstr "Das Passwort muss zwischen %min% und %max% Zahlen enthalten."

#: ../src/Rule/GuessableDataRule.php:151
msgid "The password cannot contain words that are easy to guess."
msgstr "Das Passwort darf keine Wörter enthalten, die leicht zu erraten sind."

#: ../src/Rule/HaveIBeenPwnedRule.php:200
msgid "The password must appear at least once in data breaches."
msgid_plural "The password must appear at least %count% times in data breaches."
msgstr[0] "Das Passwort muss mindestens einmal in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."
msgstr[1] "Dass Passwort muss mindestens %count% Mal in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."

#: ../src/Rule/HaveIBeenPwnedRule.php:208
msgid "The password cannot appear in data breaches."
msgstr "Das Passwort darf nicht in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."

#: ../src/Rule/HaveIBeenPwnedRule.php:214
msgid "The password must appear at most once in data breaches."
msgid_plural "The password must appear at most %count% times in data breaches."
msgstr[0] "Das Passwort darf maximal einmal in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."
msgstr[1] "Das Passwort darf maximal %count% Mal in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."

#: ../src/Rule/HaveIBeenPwnedRule.php:222
msgid "The password must appear exactly once in data breaches."
msgid_plural "The password must appear exactly %count% times in data breaches."
msgstr[0] "Das Passwort muss genau einmal in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."
msgstr[1] "Das Passwort muss genau %count% Mal in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."

#: ../src/Rule/HaveIBeenPwnedRule.php:229
msgid "The password must appear between %min% and %max% times in data breaches."
msgstr "Das Passwort muss zwischen %min% und %max% Mal in Verzeichnissen zu Verletzungen des Schutzes personenbezogener Daten vorkommen."

#: ../src/Rule/LengthRule.php:125
msgid "The password must contain at least one character."
msgid_plural "The password must contain at least %count% characters."
msgstr[0] "Das Passwort muss mindestens ein Zeichen enthalten."
msgstr[1] "Das Passwort muss mindestens %count% Zeichen enthalten."

#: ../src/Rule/LengthRule.php:133
msgid "The password cannot contain characters."
msgstr "Das Passwort darf keine Zeichen enthalten."

#: ../src/Rule/LengthRule.php:139
msgid "The password must contain at most one character."
msgid_plural "The password must contain at most %count% characters."
msgstr[0] "Das Passwort darf maximal ein Zeichen enthalten."
msgstr[1] "Das Passwort darf maximal %count% Zeichen enthalten."

#: ../src/Rule/LengthRule.php:147
msgid "The password must contain exactly one character."
msgid_plural "The password must contain exactly %count% characters."
msgstr[0] "Das Passwort muss genau ein Zeichen enthalten."
msgstr[1] "Das Passwort muss genau %count% Zeichen enthalten."

#: ../src/Rule/LengthRule.php:154
msgid "The password must contain between %min% and %max% characters."
msgstr "Das Passwort muss zwischen %min% und %max% Zeichen enthalten."

#: ../src/Rule/LowerCaseRule.php:150
msgid "The password must contain at least one lower case letter."
msgid_plural "The password must contain at least %count% lower case letters."
msgstr[0] "Das Passwort muss mindestens einen Kleinbuchstaben enthalten."
msgstr[1] "Das Passwort muss mindestens %count% Kleinbuchstaben enthalten."

#: ../src/Rule/LowerCaseRule.php:158
msgid "The password cannot contain lower case letters."
msgstr "Das Passwort darf keine Kleinbuchstaben enthalten."

#: ../src/Rule/LowerCaseRule.php:164
msgid "The password must contain at most one lower case letter."
msgid_plural "The password must contain at most %count% lower case letters."
msgstr[0] "Das Passwort darf maximal einen Kleinbuchstaben enthalten."
msgstr[1] "Das Passwort darf maximal %count% Kleinbuchstaben enthalten."

#: ../src/Rule/LowerCaseRule.php:172
msgid "The password must contain exactly one lower case letter."
msgid_plural "The password must contain exactly %count% lower case letters."
msgstr[0] "Das Passwort muss genau einen Kleinbuchstaben enthalten."
msgstr[1] "Das Passwort muss genau %count% Kleinbuchstaben enthalten."

#: ../src/Rule/LowerCaseRule.php:179
msgid "The password must contain between %min% and %max% lower case letters."
msgstr "Das Passwort muss zwischen %min% und %max% Kleinbuchstaben enthalten."

#: ../src/Rule/NoReuseRule.php:153
msgid "Formerly used passwords cannot be reused."
msgstr "Zuvor genutzte Passwörter können nicht wieder verwendet werden."

#: ../src/Rule/NoReuseRule.php:158
msgid "The most recently used password cannot be reused."
msgid_plural "The %count% most recently used passwords cannot be reused."
msgstr[0] "Das zuletzt verwendet Passwort kann nicht wieder verwendet werden."
msgstr[1] "Die %count% zuletzt verwendeten Passwörter können nicht wieder verwendet werden."

#: ../src/Rule/NotSetInIntervalRule.php:153
msgid "The password must have been set before %date%."
msgstr "Das Passwort muss vor %date% festgelegt worden sein."

#: ../src/Rule/NotSetInIntervalRule.php:160
msgid "The password must have been set after %date%."
msgstr "Das Passwort muss nach %date% festgelegt worden sein."

#: ../src/Rule/NotSetInIntervalRule.php:169
msgid "The password must have been set before or after %date%."
msgstr "Das Passwort muss vor oder nach %date% festgelegt worden sein."

#: ../src/Rule/NotSetInIntervalRule.php:175
msgid "The password must have been set before %min% or after %max%."
msgstr "Das Passwort muss vor %min% oder nach %max% festgelegt worden sein."

#: ../src/Rule/SymbolRule.php:15
msgid "The password must contain at least one symbol (%characters%)."
msgid_plural "The password must contain at least %count% symbols (%characters%)."
msgstr[0] "Das Passwort muss mindestens ein Symbol (%characters%) enthalten."
msgstr[1] "Das Passwort muss mindestens %count% Symbole (%characters%) enthalten."

#: ../src/Rule/SymbolRule.php:26
msgid "The password cannot contain symbols (%characters%)."
msgstr "Das Passwort darf keine Symbole (%characters%) enthalten."

#: ../src/Rule/SymbolRule.php:33
msgid "The password must contain at most one symbol (%characters%)."
msgid_plural "The password must contain at most %count% symbols (%characters%)."
msgstr[0] "Das Passwort darf maximal ein Symbol (%characters%) enthalten."
msgstr[1] "Das Passwort darf maximal %count% Symbole (%characters%) enthalten."

#: ../src/Rule/SymbolRule.php:44
msgid "The password must contain exactly one symbol (%characters%)."
msgid_plural "The password must contain exactly %count% symbols (%characters%)."
msgstr[0] "Das Passwort muss genau ein Symbol (%characters%) enthalten."
msgstr[1] "Das Passwort muss genau %count% Symbole (%characters%) enthalten."

#: ../src/Rule/SymbolRule.php:54
msgid "The password must contain between %min% and %max% symbols (%characters%)."
msgstr "Das Passwort muss zwischen %min% und %max% Symbole (%characters%) enthalten."

#: ../src/Rule/UpperCaseRule.php:150
msgid "The password must contain at least one upper case letter."
msgid_plural "The password must contain at least %count% upper case letters."
msgstr[0] "Das Passwort muss mindestens einen Großbuchstaben enthalten."
msgstr[1] "Das Passwort muss mindestens %count% Großbuchstaben enthalten."

#: ../src/Rule/UpperCaseRule.php:158
msgid "The password cannot contain upper case letters."
msgstr "Das Passwort darf keine Großbuchstaben enthalten."

#: ../src/Rule/UpperCaseRule.php:164
msgid "The password must contain at most one upper case letter."
msgid_plural "The password must contain at most %count% upper case letters."
msgstr[0] "Das Passwort darf maximal einen Großbuchstaben enthalten."
msgstr[1] "Das Passwort darf maximal %count% Großbuchstaben enthalten."

#: ../src/Rule/UpperCaseRule.php:172
msgid "The password must contain exactly one upper case letter."
msgid_plural "The password must contain exactly %count% upper case letters."
msgstr[0] "Das Passwort muss genau einen Großbuchstaben enthalten."
msgstr[1] "Das Passwort muss genau %count% Großbuchstaben enthalten."

#: ../src/Rule/UpperCaseRule.php:179
msgid "The password must contain between %min% and %max% upper case letters."
msgstr "Das Passwort muss zwischen %min% und %max% Großbuchstaben enthalten."
